import "kmeans_final.daph";

seed = 126178;
num_points = 100;

// Define the centers of the clusters
center1 = [1.0, 2.0];
center2 = [3.0, 4.0];

noise1 = rand(num_points, 2, -0.5, 0.5, 1.0, seed);
noise2 = rand(num_points, 2, -0.5, 0.5, 1.0, seed);

cluster1 = noise1 + t(center1);
cluster2 = noise2 + t(center2);

data = rbind(cluster1, cluster2);

k = 3;
runs = 100;
max_iter = 100;
eps = 0.000001;
is_verbose = true;
avg_sample_size_per_centroid = 3;

X = readMatrix($inPath);

C, Y = kmeans_final.m_kmeans(as.matrix<f64>(X), k, runs, max_iter, eps, is_verbose, avg_sample_size_per_centroid, seed);
print(C);
